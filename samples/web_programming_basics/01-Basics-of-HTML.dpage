            "senderId": "nobody"
        },
        "6ee3226b-cae0-4b7c-b5c7-968ae8bf4986": {
            "id": "6ee3226b-cae0-4b7c-b5c7-968ae8bf4986",
            "source": "Makes sense. It’s like a family structure.",
            "parentId": "b6bdf298-7abb-4926-a1b3-869dd56c543b",
            "childrenIds": [
                "488b7567-97f3-4e8c-9e43-27c261bbcae1"
