        "9cd4705f-ac7e-47f1-b782-85434ee4b1e7": {
            "id": "9cd4705f-ac7e-47f1-b782-85434ee4b1e7",
            "source": "What is DOM?",
            "type": "message",
            "senderId": "user",
            "directives": [],
            "childrenIds": [
                "e4878b26-89ee-471d-a181-9fecf93924ac"
            ],
            "shortcutsTo": [],
            "parentId": "131faf10-5977-49bb-b590-da2ee22463ce"
        },
        "e4878b26-89ee-471d-a181-9fecf93924ac": {
            "id": "e4878b26-89ee-471d-a181-9fecf93924ac",
            "source": "DOM stands for \"Document Object Model.\" In the context of HTML (Hypertext Markup Language) and web development, the DOM refers to the structured representation of a web page's content, layout, and elements in memory.\n>\nIn summary, the DOM is a programming interface that provides a structured way to interact with and manipulate the elements and content of an HTML document, facilitating dynamic and interactive web development.",
            "type": "message",
            "senderId": "instructor",
            "directives": [],
            "childrenIds": [],
            "shortcutsTo": [],
            "parentId": "9cd4705f-ac7e-47f1-b782-85434ee4b1e7",
            "isAIContent": false
