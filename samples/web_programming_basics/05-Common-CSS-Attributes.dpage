        "fac184b3-9270-43ba-bcea-2ffa521cde31": {
            "id": "fac184b3-9270-43ba-bcea-2ffa521cde31",
            "source": "Great. Now, onto backgrounds. How can I change an element's background color?",
            "type": "message",
            "senderId": "user",
            "directives": [],
            "childrenIds": [
                "acc23ea3-5136-497d-9186-f2dda3be910f"
