                        [
                            0,
                            2
                        ],
                        [
                            -1,
                            1
                        ],
                        [
                            1,
                            "  min-width: 400px;\n"
                        ],
                        [
                            0,
                            1
                        ]
                    ]
                }
            },
            "editorPointers": [
                {
                    "type": "pointer",
                    "filename": "file_2.css",
                    "startLineNumber": 3,
                    "startColumn": 3,
                    "endLineNumber": 3,
                    "endColumn": 20
                }
            ]
